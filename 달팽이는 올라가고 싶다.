import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int A = sc.nextInt();
		int B = sc.nextInt();
		int V = sc.nextInt();
		
        int day = (V - B) / (A - B);
        
		if((V - B) % (A - B) != 0)
		{
		    day++;
		}
		
		System.out.println(day);
   
	    sc.close();
	}   
}

// 이 문제의 경우 JAVA8로 제출해야 시간초과가 안뜸.

// 혼자만의 풀이가 아니니 추후 다시 풀어볼것..
